<?xml version = "1.0" encoding = "utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="es.jaime.gateway.orders.newmodel.execution.sell.OrderSell" table="sell_orders">
        <composite-id name="orderId" class="es.jaime.gateway.orders.newmodel._shared.OrderId" access="field">
            <key-property column="orderId" name="value" length="10" access="field"/>
        </composite-id>

        <component name="clientId" class="es.jaime.gateway.orders.newmodel._shared.OrderI.OrderClientId" access="field">
            <property column="clientId" name="value" type="string" access="field"/>
        </component>

        <component name="date" class="es.jaime.gateway.orders.newmodel._shared.OrderDate" access="field">
            <property column="date" name="value" type="string" access="field"/>
        </component>

        <component name="type" class="es.jaime.gateway.orders.newmodel._shared.OrderTicker" access="field">
            <property column="type" name="value" type="string" access="field"/>
        </component>

        <component name="state" class="es.jaime.gateway.orders.newmodel._shared.OrderState" access="field">
            <property column="state" name="value" type="string" access="field"/>
        </component>

        <component name="ticker" class="es.jaime.gateway.orders.newmodel._shared.OrderTicker" access="field">
            <property column="ticker" name="value" type="string" access="field"/>
        </component>

        <component name="quantity" class="es.jaime.gateway.orders.newmodel._shared.OrderQuantity" access="field">
            <property column="quantity" name="value" type="int" access="field"/>
        </component>

        <component name="executionPrice" class="es.jaime.gateway.orders.newmodel._shared.OrderExecutionPrice" access="field">
            <property column="executionPrice" name="value" type="double" access="field"/>
        </component>

        <component name="positionIdToSell" class="es.jaime.gateway.orders.newmodel._shared.OrderPositionIdToSell" access="field">
            <property column="positionIdToSell" name="value" type="string" access="field"/>
        </component>
    </class>
</hibernate-mapping>
