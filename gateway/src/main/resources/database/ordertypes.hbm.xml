<?xml version = "1.0" encoding = "utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="es.jaime.gateway.orders.ordertypes.domain.OrderType" table="ordertypes">
        <composite-id name="orderTypeId" class="es.jaime.gateway.orders.ordertypes.domain.OrderTypeId" access="field">
            <key-property column="orderTypeId" name="value" length="20" access="field"/>
        </composite-id>

        <component name="name" class="es.jaime.gateway.orders.ordertypes.domain.OrderTypeName" access="field">
            <property column="name" name="value" type="string" access="field"/>
        </component>
    </class>
</hibernate-mapping>
